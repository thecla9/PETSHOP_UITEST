    function solution(n) {
      // Get the first digit by dividing by 10 and using Math.floor to round down
      let firstDigit = Math.floor(n / 10);
      
      // Get the second digit using the modulus operator
      let secondDigit = n % 10;
      
      // Return the sum of the two digits
      return firstDigit + secondDigit;
  }
  
  // Example usage:
  console.log(solution(29)); // Output: 11
  console.log(solution(11)); // Output: 2
  